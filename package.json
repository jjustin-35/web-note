  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tsconfig/svelte": "^5.0.2",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.24",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3"
  },
  "type": "module",
  "dependencies": {
    "@auth/core": "^0.37.4",
    "@auth/sveltekit": "^1.7.4",
    "@prisma/client": "^6.3.1",
    "@supabase/supabase-js": "^2.39.0",
    "prisma": "^6.3.1"
  }
